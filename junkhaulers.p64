	ret._sprite=1
	ret.thrust=.25
	ret.name="command"
	ret.attach_sides = {
		["1,0"] = true,   -- right
		["-1,0"] = true, -- left
		["0,-1"] = false,  -- up
		["0,1"] = true   -- down
	}
	ret.basekill=ret.kill
	ret.kill=function(self)
		if(self:basekill()) then
			game:death()
